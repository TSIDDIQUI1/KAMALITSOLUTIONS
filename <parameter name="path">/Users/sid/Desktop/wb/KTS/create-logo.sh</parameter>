    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  <ellipse fill="none" stroke="#38bdf8" stroke-width="1.5" opacity="0.6" cx="100" cy="100" rx="80" ry="30"/>
  <ellipse fill="none" stroke="#38bdf8" stroke-width="1.5" opacity="0.6" cx="100" cy="100" rx="70" ry="70"/>
  <ellipse fill="none" stroke="#38bdf8" stroke-width="1.5" opacity="0.6" cx="100" cy="100" rx="30" ry="80"/>
  <circle fill="#0d1117" stroke="#38bdf8" stroke-width="2" cx="100" cy="100" r="45"/>
  <text x="100" y="118" text-anchor="middle" fill="#38bdf8" font-family="Arial Black, sans-serif" font-size="55" font-weight="900" filter="url(#glow)">K</text>
  <line stroke="#38bdf8" stroke-width="2" stroke-linecap="round" stroke-dasharray="8 4" x1="100" y1="55" x2="100" y2="25"/>
  <line stroke="#38bdf8" stroke-width="2" stroke-linecap="round" stroke-dasharray="8 4" x1="100" y1="145" x2="100" y2="175"/>
  <line stroke="#38bdf8" stroke-width="2" stroke-linecap="round" stroke-dasharray="8 4" x1="55" y1="100" x2="25" y2="100"/>
  <line stroke="#38bdf8" stroke-width="2" stroke-linecap="round" stroke-dasharray="8 4" x1="145" y1="100" x2="175" y2="100"/>
  <circle fill="#38bdf8" filter="url(#glow)" cx="30" cy="30" r="5"/>
  <circle fill="#38bdf8" filter="url(#glow)" cx="170" cy="30" r="4"/>
  <circle fill="#38bdf8" filter="url(#glow)" cx="30" cy="170" r="4"/>
  <circle fill="#38bdf8" filter="url(#glow)" cx="170" cy="170" r="5"/>
  <line stroke="#38bdf8" stroke-width="1" opacity="0.5" x1="30" y1="30" x2="55" y2="55"/>
  <line stroke="#38bdf8" stroke-width="1" opacity="0.5" x1="170" y1="30" x2="145" y2="55"/>
  <line stroke="#38bdf8" stroke-width="1" opacity="0.5" x1="30" y1="170" x2="55" y2="145"/>
  <line stroke="#38bdf8" stroke-width="1" opacity="0.5" x1="170" y1="170" x2="145" y2="145"/>
  <text fill="#38bdf8" font-family="Courier New, monospace" font-size="10" opacity="0.6" x="45" y="40">1</text>
  <text fill="#38bdf8" font-family="Courier New, monospace" font-size="10" opacity="0.6" x="155" y="40">0</text>
  <text fill="#38bdf8" font-family="Courier New, monospace" font-size="10" opacity="0.6" x="45" y="165">1</text>
  <text fill="#38bdf8" font-family="Courier New, monospace" font-size="10" opacity="0.6" x="155" y="165">0</text>
  <circle fill="none" stroke="#38bdf8" stroke-width="0.5" opacity="0.3" cx="100" cy="100" r="90"/>
</svg>
